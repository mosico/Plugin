{
    "name": "Coupon Digger",
    "version": "1.1.2013.6091",
    "manifest_version": 2,
    "description": "Easily find online coupon codes to use at all your favorite stores.",
    "icons": {"16": "images/16.png", "48": "images/48.png", "128": "images/128.png"},
    "permissions": ["tabs", "http://*/*", "https://*/*"],
    "background": {
        "page": "background.html"
    },
	"options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery.js", "const.js", "mix.js", "checkLoad.js", "ga.js"],
            "run_at": "document_start"
        }
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "minimum_chrome_version": "18.0"
}